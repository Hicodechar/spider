 		} else if (cnt) {
<span class="p_del">-			new_prog = bpf_patch_insn_single(env-&gt;prog, 0,</span>
<span class="p_del">-							 insn_buf, cnt);</span>
<span class="p_add">+			new_prog = bpf_patch_insn_data(env, 0, insn_buf, cnt);</span>
 			if (!new_prog)
 				return -ENOMEM;
<span class="p_add">+</span>
