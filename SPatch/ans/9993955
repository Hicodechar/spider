	export _id=&#39;59e22f1e0b9a9376ecec5c6b&#39;
	export _rt=&#39;/result/will-it-scale/50%-process-context_switch1-performance/lkp-bdw-ep3d/debian-x86_64-2016-08-31.cgz/x86_64-rhel-7.2/gcc-6/c4c3c3c2d00826c88b5c02c20e80704664424b9b&#39;
	export user=&#39;lkp&#39;
	export head_commit=&#39;e8c8801d85e37242794e28080ab2aa2c1aeec7e0&#39;
