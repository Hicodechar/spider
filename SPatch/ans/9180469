<span class="p_add">+	sb_start_pagefault(VFS_I(ip)-&gt;i_sb);</span>
<span class="p_add">+	file_update_time(vma-&gt;vm_file);</span>
<span class="p_add">+	xfs_ilock(ip, XFS_MMAPLOCK_SHARED);</span>
